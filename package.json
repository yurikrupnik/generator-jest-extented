{
  "name": "generator-jest-extended",
  "version": "1.1.5",
  "description": "extend jest generator",
  "main": "dist/main.js",
  "module": "dist/main.js",
  "scripts": {
    "lint:templates": "ejslint app/templates**",
    "prsetest": "eslint app",
    "test": "jest app",
    "test:watch": "jest app--watch",
    "test:coveralls": "jest app --coverage --coverageReporters=text-lcov | coveralls",
    "prebuild": "rimraf dist",
    "build": "webpack"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yurikrupnik/generator-jest-extended.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/yurikrupnik/generator-jest-extended/issues"
  },
  "homepage": "https://github.com/yurikrupnik/generator-jest-extended#readme",
  "engines": {
    "npm": ">= 6.0.0"
  },
  "jest": {
    "collectCoverage": true,
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "templates"
    ]
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "node",
    "module",
    "scaffold",
    "cli",
    "yeoman-generator",
    "testing",
    "jest",
    "coverage",
    "reports"
  ],
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "codecov": "^3.3.0",
    "coveralls": "^3.0.3",
    "ejs-lint": "^0.3.0",
    "eslint": "^5.3.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.17.2",
    "husky": "^2.2.0",
    "jest": "^24.7.1",
    "lint-staged": "^8.1.5",
    "semantic-release": "^15.13.3",
    "semantic-release-cli": "^4.1.2",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.0",
    "webpack-node-externals": "^1.7.2",
    "yeoman-assert": "^3.1.1",
    "yeoman-test": "^1.9.1"
  },
  "dependencies": {
    "yeoman-generator": "^3.2.0"
  }
}
