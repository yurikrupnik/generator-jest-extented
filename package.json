{
  "name": "generator-jest-extented",
  "version": "1.0.0",
  "description": "extend jest generator",
  "main": "index.js",
  "scripts": {
    "lint:templates": "ejslint app/templates**",
    "pretest": "eslint ./app",
    "test": "jest",
    "test:watch": "jest --watch",
    "coverage": "jest --coverage --coverageReporters=text-lcov | coveralls",
    "build": "webpack",
    "clean": "rimraf dist/"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yurikrupnik/generator-jest-extented.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/yurikrupnik/generator-jest-extented/issues"
  },
  "homepage": "https://github.com/yurikrupnik/generator-jest-extented#readme",
  "engines": {
    "npm": ">= 6.0.0"
  },
  "jest": {
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "templates"
    ],
    "collectCoverage": true
  },
  "files": [
    "app"
  ],
  "keywords": [
    "node",
    "module",
    "scaffold",
    "cli",
    "yeoman-generator",
    "testing",
    "jest",
    "coverage",
    "reports"
  ],
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "coveralls": "^3.0.3",
    "ejs-lint": "^0.3.0",
    "eslint": "^5.16.0",
    "eslint-loader": "^2.1.2",
    "husky": "^1.3.1",
    "jest": "^24.7.1",
    "lint-staged": "^8.1.5",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.0",
    "webpack-node-externals": "^1.7.2",
    "yeoman-assert": "^3.1.1",
    "yeoman-test": "^1.9.1"
  },
  "dependencies": {
    "yeoman-generator": "^3.2.0"
  }
}
